{
	"env": {
		"es6": true,
		"node": true
	},
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"@typescript-eslint"
	],
	"parserOptions": {
		"ecmaVersion": 9,
		"sourceType": "module",
		"impliedStrict": true,
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": [
		"plugin:react/recommended"
		// "plugin:@typescript-eslint/recommended"
	],
	"rules": {
		"block-scoped-var": 2,
		"complexity": [
			"warn",
			{
				"max": 20
			}
		],
		"curly": [
			"error",
			"multi-line"
		],
		"dot-location": 0,
		"dot-notation": 2,
		"default-case": 2,
		"eqeqeq": 2,
		"eol-last": [
			"error",
			"always"
		],
		"no-alert": 2,
		"no-caller": 2,
		"no-console": [
			1,
			{
				"allow": [
					"warn",
					"error"
				]
			}
		],
		"no-else-return": 2,
		"no-empty-function": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-extend-native": 2,
		"no-extra-bind": 2,
		"no-extra-label": 2,
		"no-floating-decimal": 2,
		"no-implied-eval": 2,
		"no-invalid-this": 2,
		"no-iterator": 2,
		"no-labels": 2,
		"no-lone-blocks": 2,
		"no-loop-func": 2,
		"no-magic-numbers": 0,
		"no-multi-spaces": 2,
		"no-multi-str": 2,
		"no-new": 2,
		"no-new-func": 2,
		"no-new-wrappers": 2,
		"no-proto": 2,
		"no-return-await": 2,
		"no-script-url": 2,
		"no-self-compare": 2,
		"no-sequences": 2,
		"no-throw-literal": 2,
		"no-unmodified-loop-condition": 2,
		"no-unused-expressions": 2,
		"no-unused-vars": 2,
		"no-useless-call": 2,
		"no-useless-return": 2,
		"no-void": 2,
		"vars-on-top": 2,
		"yoda": [
			"error",
			"never"
		],
		"no-catch-shadow": 2,
		"no-label-var": 2,
		"no-use-before-define": 0,
		"array-bracket-newline": [
			"error",
			{
				"multiline": true
			}
		],
		"array-bracket-spacing": 2,
		"array-element-newline": [
			"error",
			{
				"multiline": true
			}
		],
		"arrow-body-style": ["error", "as-needed"],
		"block-spacing": [
			"error",
			"always"
		],
		"brace-style": [
			"error",
			"1tbs",
			{
				"allowSingleLine": true
			}
		],
		"comma-dangle": [
			"error",
			"always-multiline"
		],
		"comma-spacing": [
			"error",
			{
				"after": true
			}
		],
		"func-call-spacing": [
			"error",
			"never"
		],
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"key-spacing": [
			"error",
			{
				"afterColon": true
			}
		],
		"keyword-spacing": [
			"error",
			{
				"before": true,
				"after": true
			}
		],
		"linebreak-style": [
			"off",
			"unix"
		],
		"max-len": [
			"warn",
			{
				"code": 150
			}
		],
		"no-trailing-spaces": 2,
		"no-mixed-spaces-and-tabs": 2,
		"prefer-template": "error",
		"semi-spacing": 2,
		"quotes": [
			"error",
			"single",
			{
				"avoidEscape": true
			}
		],
		"jsx-quotes": [
			"error",
			"prefer-single"
		],
		"semi": [
			"error",
			"never"
		],
		"semi-style": 2,
		"space-infix-ops": 2,
		"space-before-blocks": 2,
		"spaced-comment": 2,
		"no-var": 2,
		"prefer-const": 1,
		"no-multiple-empty-lines": [
			"error",
			{
				"max": 1
			}
		],
		"no-mixed-operators": 0,
		"object-curly-spacing": [
			"error",
			"always"
		],
		"react/jsx-indent": [
			2,
			"tab"
		],
		"react/jsx-equals-spacing": [
			2,
			"never"
		],
		"react/jsx-tag-spacing": [
			2
		],
		"react/jsx-max-props-per-line": [
			2
		],
		"react/jsx-first-prop-new-line": [
			2,
			"multiline-multiprop"
		],
		"react/jsx-closing-tag-location": 2,
		"react/jsx-closing-bracket-location": 2,
		"react/jsx-one-expression-per-line": [
			2,
			{
				"allow": "single-child"
			}
		],
		"@typescript-eslint/no-unused-vars": "error",
		"@typescript-eslint/indent": ["error", "tab"],
		"@typescript-eslint/member-delimiter-style": [
			"error",
			{
				"multiline": {
					"delimiter": "none",
					"requireLast": false
				}
			}
		],
		"@typescript-eslint/class-name-casing": "error"
	}
}